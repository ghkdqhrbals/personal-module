# 로컬 개발용 설정 파일
# 이 파일은 git에 커밋되지 않습니다 (.gitignore에 포함됨)
# 실제 키 값으로 변경하여 사용하세요

spring:
  config:
    import: []  # AWS Secrets Manager 비활성화

openai:
  enabled: true
  api:
    key: sk-proj-YOUR_OPENAI_KEY_HERE

serpapi:
  key: YOUR_SERPAPI_KEY_HERE

# 로깅 레벨 조정 (선택)
logging:
  level:
    org.ghkdqhrbals: DEBUG
    org.springframework.cloud.aws: DEBUG

